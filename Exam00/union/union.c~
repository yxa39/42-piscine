#include <unistd.h>

int main(int argc, char **argv)
{
	int i;
	int bool;

	i = 0;
	bool = 0;
	if (argument == 3)
	{
		while (argv[1][i])
		{
			j = i-1;
			while (argv[1][j])
			{
				if (argv[1][i] == argv[1][i])
					bool = 1;
				j --;
			}
			if (bool == 0)
			{
				write(1, &argv[1][j], 1);
			}
			i++;
		}
		
	}
	write(1, "\n", 1);
}
