#include<unistd.h>

int putchar(char c){
	write(1,&c,1);
    return (0);
}

int main(){
	letter_1 = 'a';
	letter_2 = 'B';
	while(letter_2 <= 'Z'){
		putchar(letter_1);
		putchar(letter_2);
		letter_1++;
		letter_2++;
	}
	putchar('\n');
}
