#include <unistd.h>

int main(int argc, char **argv)
{
	if (argc == 2)
	{
		while (*argv[1])
		{
			if ('a' =< *argv[1] <= 'z')
				write(1, argv[1], 1);
		}
	}
	write(1, "\n", 1);
}
